{
  "name": "typescript-orm-postgres",
  "version": "1.0.0",
  "description": "Rest API using Node.js, TypeScript ORM Sequelize, Express, Postgres",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=development concurrently  \"npm run watch-dev\"",
    "test": "jest --watchAll --no-cache --detectOpenHandles",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "tsc -w & nodemon --watch \"dist/**/*\"  -e js ./dist/server.js",
    "start": "tsc -w & node dist/server.js"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "orm",
    "sequelize",
    "express",
    "postgres",
    "postgresql",
    "restapi",
    "rest",
    "api",
    "crud"
  ],
  "author": "bezkoder",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/http-status": "^0.2.30",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/nodemon": "^1.19.6",
    "@types/supertest": "^6.0.2",
    "bcrypt": "^5.1.1",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "http-status": "^2.0.0",
    "http-status-ts": "^2.0.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.7",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "winston": "^3.16.0"
  }
}
